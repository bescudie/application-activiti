<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/DocWorkflow">
  <collaboration id="Collaboration">
    <participant id="DocumentWorkflowPool" name="Article Publishing" processRef="DocWorkflow"></participant>
  </collaboration>
  <process id="DocWorkflow" name="Document Workflow" isExecutable="true" activiti:candidateStarterGroups="xwiki:XWiki.Writers">
    <documentation>Document Workflow</documentation>
    <laneSet id="laneSet_DocWorkflow">
      <lane id="lane1" name="Reporter/Collaborator/Contributor">
        <flowNodeRef>startevent1</flowNodeRef>
        <flowNodeRef>DescribeArticle</flowNodeRef>
        <flowNodeRef>WriteArticle</flowNodeRef>
      </lane>
      <lane id="lane2" name="Chief Editor">
        <flowNodeRef>ApproveArticleCreation</flowNodeRef>
        <flowNodeRef>gateway1</flowNodeRef>
      </lane>
      <lane id="lane3" name="Junior Editor">
        <flowNodeRef>usertask4</flowNodeRef>
      </lane>
      <lane id="lane4" name="Editor">
        <flowNodeRef>usertask5</flowNodeRef>
        <flowNodeRef>usertask6</flowNodeRef>
        <flowNodeRef>exclusivegateway1</flowNodeRef>
        <flowNodeRef>usertask6</flowNodeRef>
        <flowNodeRef>usertask5</flowNodeRef>
        <flowNodeRef>exclusivegateway1</flowNodeRef>
        <flowNodeRef>scripttask2</flowNodeRef>
        <flowNodeRef>parallelgateway1</flowNodeRef>
        <flowNodeRef>parallelgateway2</flowNodeRef>
      </lane>
      <lane id="lane5" name="Publishing">
        <flowNodeRef>scripttask1</flowNodeRef>
        <flowNodeRef>endevent1</flowNodeRef>
        <flowNodeRef>scripttask3</flowNodeRef>
      </lane>
    </laneSet>
    <startEvent id="startevent1" name="Start"></startEvent>
    <userTask id="DescribeArticle" name="Write Short Article Description" activiti:candidateGroups="xwiki:XWiki.Writers">
      <documentation>Fill article information</documentation>
      <extensionElements>
        <activiti:formProperty id="short description" name="Article Short Description" type="textArea" variable="Article Short Description" required="true"></activiti:formProperty>
        <activiti:formProperty id="Article Category" name="Article Category" type="enum" variable="type" required="true">
          <activiti:value id="IT" name="IT"></activiti:value>
          <activiti:value id="Politics" name="Politics"></activiti:value>
          <activiti:value id="Sports" name="Sports"></activiti:value>
          <activiti:value id="Financial" name="Financial"></activiti:value>
        </activiti:formProperty>
        <activiti:taskListener event="create" class="org.xwiki.activiti.documentWorkflow.DescribeArticleTaskListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="DescribeArticle"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="DescribeArticle" targetRef="ApproveArticleCreation"></sequenceFlow>
    <userTask id="ApproveArticleCreation" name="Approve Article Creation" activiti:candidateGroups="xwiki:XWiki.Writers">
      <documentation>Approve article creation</documentation>
      <extensionElements>
        <activiti:formProperty id="Article Name" name="Article Name" type="XWikiDocumentReference" variable="documentReference" writable="false"></activiti:formProperty>
        <activiti:formProperty id="Article Space" name="Article Space" type="string" variable="documentSpace" writable="false"></activiti:formProperty>
        <activiti:formProperty id="Article Category" name="Article Category" type="enum" variable="type" writable="false"></activiti:formProperty>
        <activiti:formProperty id="Article Author" name="Article Author" type="XWikiUsernameReference" variable="usernameReference" writable="false"></activiti:formProperty>
        <activiti:formProperty id="Allow" name="Allow creation of Article" type="boolean" variable="Allow" required="true"></activiti:formProperty>
        <activiti:taskListener event="create" class="org.xwiki.activiti.documentWorkflow.ApproveArticleCreationTaskListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="WriteArticle" name="Write Article" activiti:candidateGroups="xwiki:XWiki.Writers">
      <documentation>Write article</documentation>
      <extensionElements>
        <activiti:formProperty id="Article Name" name="Article Name" type="XWikiDocumentReference" variable="documentReference" writable="false"></activiti:formProperty>
        <activiti:taskListener event="create" class="org.xwiki.activiti.documentWorkflow.WriteArticleTaskListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <scriptTask id="scripttask1" name="Publish Article" scriptFormat="groovy" activiti:autoStoreVariables="true">
      <script>System.out.println("Publishing Article");</script>
    </scriptTask>
    <sequenceFlow id="flow16" sourceRef="scripttask1" targetRef="endevent1"></sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <userTask id="usertask6" name="Review Two" activiti:candidateGroups="xwiki:XWiki.Writers">
      <documentation>Reivew 2</documentation>
      <extensionElements>
        <activiti:formProperty id="Article Name" name="Article Name" type="XWikiDocumentReference" variable="documentReference" writable="false"></activiti:formProperty>
        <activiti:formProperty id="Article Creator" name="Article Creator" type="XWikiUsernameReference" variable="usernameReference" writable="false"></activiti:formProperty>
        <activiti:formProperty id="Review Two Passed" name="Review Two Passed" type="boolean" variable="review2" required="true"></activiti:formProperty>
        <activiti:formProperty id="Review Two Comment" name="Review Two Comment" type="textArea" variable="review2Comment"></activiti:formProperty>
        <activiti:taskListener event="create" class="org.xwiki.activiti.documentWorkflow.ReviewTaskListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="usertask5" name="Review One" activiti:candidateGroups="xwiki:XWiki.Writers">
      <documentation>Review 1</documentation>
      <extensionElements>
        <activiti:formProperty id="Article Name" name="Article Name" type="XWikiDocumentReference" variable="documentReference" writable="false"></activiti:formProperty>
        <activiti:formProperty id="Article Creator" name="Article Creator" type="XWikiUsernameReference" variable="usernameReference" writable="false"></activiti:formProperty>
        <activiti:formProperty id="Review One Passed" name="Review One Passed" type="boolean" variable="review1" required="true"></activiti:formProperty>
        <activiti:formProperty id="Review One Comment" name="Review One Comment" type="textArea" variable="review1Comment"></activiti:formProperty>
        <activiti:taskListener event="create" class="org.xwiki.activiti.documentWorkflow.ReviewTaskListener"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow15" name="Approved" sourceRef="exclusivegateway1" targetRef="scripttask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${publish == true}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow14" name="Not Approved" sourceRef="exclusivegateway1" targetRef="usertask4"></sequenceFlow>
    <userTask id="usertask4" name="Improve Article" activiti:candidateGroups="xwiki:XWiki.Writers">
      <documentation>Improve article</documentation>
      <extensionElements>
        <activiti:formProperty id="Article Name" name="Article Name" type="XWikiDocumentReference" variable="documentName" writable="false"></activiti:formProperty>
        <activiti:formProperty id="Review Two Comment" name="Review Two Comment" type="textArea" variable="review2Comment" writable="false"></activiti:formProperty>
        <activiti:formProperty id="Review One Comment" name="Review One Comment" type="textArea" variable="review1Comment" writable="false"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <exclusiveGateway id="gateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow3" sourceRef="ApproveArticleCreation" targetRef="gateway1"></sequenceFlow>
    <sequenceFlow id="flow4" name="Approved" sourceRef="gateway1" targetRef="WriteArticle">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${Allow == true}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="WriteArticle" targetRef="usertask4"></sequenceFlow>
    <scriptTask id="scripttask2" name="Check Status" scriptFormat="groovy" activiti:autoStoreVariables="true">
      <script>if (execution.getVariable("review1") == true &amp;&amp; execution.getVariable("review2") == true){
System.out.println('BOTH are true')
execution.setVariable("publish", true)
}
else{
System.out.println('One of them is not true')
execution.setVariable("publish", false)
}</script>
    </scriptTask>
    <sequenceFlow id="flow13" sourceRef="scripttask2" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow17" sourceRef="usertask4" targetRef="parallelgateway1"></sequenceFlow>
    <parallelGateway id="parallelgateway1" name="Parallel Gateway"></parallelGateway>
    <parallelGateway id="parallelgateway2" name="Parallel Gateway"></parallelGateway>
    <sequenceFlow id="flow20" sourceRef="usertask5" targetRef="parallelgateway2"></sequenceFlow>
    <sequenceFlow id="flow21" sourceRef="usertask6" targetRef="parallelgateway2"></sequenceFlow>
    <sequenceFlow id="flow23" sourceRef="parallelgateway1" targetRef="usertask6"></sequenceFlow>
    <sequenceFlow id="flow24" sourceRef="parallelgateway1" targetRef="usertask5"></sequenceFlow>
    <sequenceFlow id="flow25" sourceRef="parallelgateway2" targetRef="scripttask2"></sequenceFlow>
    <scriptTask id="scripttask3" name="Move To Unapproved Space" scriptFormat="groovy" activiti:autoStoreVariables="true">
      <script>System.out.println("Moving to Unapproved Space");
System.out.println("Document Reference: " + execution.getVariable('documentReference'));
System.out.println("Document Name: " + execution.getVariable('documentName'));
System.out.println("Document Space: " + execution.getVariable('documentSpace'));
System.out.println("Username: " + execution.getVariable('usernameReference'));
def sourceDoc = xwiki.getDocument(execution.getVariable('documentReference'));
System.out.println("Content Author " + sourceDoc.getContentAuthor());
def targetDocRef = services.model.createDocumentReference('xwiki', 'Unapproved', sourceDoc.getName());
sourceDoc.rename(targetDocRef)
System.out.println("Document Moved");
System.out.println("CONTEXT DEBUG");
System.out.println("Context Local Username " + xcontext.getLocalUser());
System.out.println("Current Requesting Doc " + xcontext.getDoc());
System.out.println("All Context " + xcontext);
</script>
    </scriptTask>
    <sequenceFlow id="flow26" sourceRef="scripttask3" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow27" name="Not Approved" sourceRef="gateway1" targetRef="scripttask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${Allow == false}]]></conditionExpression>
    </sequenceFlow>
  </process>
