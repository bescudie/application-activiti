  <process id="demoProcess5" name="Demo Process 5" isExecutable="true" activiti:candidateStarterGroups="xwiki:XWiki.Writers">
    <documentation>Demo Process 5. Exclusive Gateway, Script Task</documentation>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <scriptTask id="scripttask3" name="Script Task" scriptFormat="groovy" activiti:autoStoreVariables="true">
      <script>System.out.println("Document Name: " + execution.getVariable('document Name'));
System.out.println("Document Space: " + execution.getVariable('document Space'));

System.out.println("Publishing Document");
def sourceDoc = xwiki.getDocument(execution.getVariable('document Name'));
def targetDocRef = services.model.createDocumentReference('xwiki', 'Published', sourceDoc.getName())
sourceDoc.rename(targetDocRef)</script>
    </scriptTask>
    <endEvent id="endevent1" name="End"></endEvent>
    <userTask id="usertask1" name="User Task" activiti:candidateGroups="xwiki:XWiki.QA">
      <documentation>Send to validation</documentation>
      <extensionElements>
        <activiti:formProperty id="comments" name="comments" type="string" variable="comments" required="true"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow6" sourceRef="scripttask3" targetRef="endevent1"></sequenceFlow>
    <userTask id="usertask2" name="User Task" activiti:candidateGroups="xwiki:XWiki.QA">
      <documentation>validate article</documentation>
      <extensionElements>
        <activiti:formProperty id="approved" name="approved" type="boolean" variable="approved"></activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow8" sourceRef="usertask1" targetRef="usertask2"></sequenceFlow>
    <sequenceFlow id="flow9" sourceRef="usertask2" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow10" name="Sent back to edit" sourceRef="exclusivegateway1" targetRef="usertask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approved == false}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow11" name="Approved" sourceRef="exclusivegateway1" targetRef="scripttask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approved == true}]]></conditionExpression>
    </sequenceFlow>
    <startEvent id="startevent1" name="Start"></startEvent>
    <sequenceFlow id="flow12" sourceRef="startevent1" targetRef="usertask1"></sequenceFlow>
  </process>
